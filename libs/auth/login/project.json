{
  "name": "auth-login",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/auth/login/src",
  "prefix": "mnp",
  "tags": ["frontend"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/auth/login/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        },
        "watch": {
          "codeCoverage": true,
          "coverage": true,
          "watch": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/auth/login/**/*.ts",
          "libs/auth/login/**/*.html"
        ]
      }
    },
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "libs/auth/login/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "library-web:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "library-web:serve:production"
        },
        "watch": {
          "watch": true
        }
      }
    },
    "deploy": {
      "executor": "@app/fire:deploy",
      "options": {
        "prerender": false,
        "ssr": false,
        "browserTarget": "library-web:build:production",
        "firebaseProject": "monorepo-d81df",
        "firebaseHostingSite": "monorepo-d81df"
      }
    }
  }
}
