{
  "name": "data-access",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/auth/data-access/src",
  "prefix": "monorepo",
  "tags": [
    "frontend"
  ],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/auth/data-access/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/auth/data-access/**/*.ts",
          "libs/auth/data-access/**/*.html"
        ]
      }
    },
    "cypress-run": {
      "executor": "@briebug/cypress-schematic:cypress",
      "options": {
        "devServerTarget": "data-access:serve",
        "configFile": "libs/auth/data-access/cypress.json"
      },
      "configurations": {
        "production": {
          "devServerTarget": "data-access:serve:production"
        }
      }
    },
    "cypress-open": {
      "executor": "@briebug/cypress-schematic:cypress",
      "options": {
        "devServerTarget": "data-access:serve",
        "watch": true,
        "headless": false,
        "configFile": "libs/auth/data-access/cypress.json"
      },
      "configurations": {
        "production": {
          "devServerTarget": "data-access:serve:production"
        }
      }
    },
    "e2e": {
      "executor": "@briebug/cypress-schematic:cypress",
      "options": {
        "devServerTarget": "data-access:serve",
        "watch": true,
        "headless": false,
        "configFile": "libs/auth/data-access/cypress.json"
      },
      "configurations": {
        "production": {
          "devServerTarget": "data-access:serve:production"
        }
      }
    }
  }
}